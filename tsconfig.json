{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "./dist",
		"noEmit": false,
		"noEmitOnError": false
	},
	"ts-node": {
		"swc": true
	},
	"exclude": ["frontend"]
}
